{
  "name": "portal",
  "scripts": {
    "ng": "ng",
    "postinstall": "cd ../../services/121-service && npm run install:dependencies-for-portal",
    "set-env-variables": "node --env-file=.env ./_set-env-variables.mjs",
    "build:deployment-configuration": "node --env-file=.env ./_build-deployment-configuration.mjs",
    "build:download-translations": "node --env-file=.env ./_build-download-translations.mjs",
    "build:manual-logout": "node --env-file=.env ./_build-manual-logout.mjs",
    "build:root-404": "shx cp ./src/404.html ./www/404.html",
    "serve:static": "swa start ./www --port 8088",
    "prestart": "npm run set-env-variables -- env=development",
    "start": "ng serve",
    "start:debug-production": "npm run build:production && npm run serve:static",
    "prebuild": "npm run set-env-variables -- env=production && npm run build:download-translations && npm run build:deployment-configuration",
    "build": "ng build --localize",
    "postbuild": "shx mv \"www/browser/*\" www/ && shx touch www/index.html && npm run build:root-404 && npm run build:manual-logout && npm run verify:build",
    "build:production": "npm run build -- --configuration=production",
    "build:stats": "npm run build:production -- --stats-json",
    "watch": "ng build --watch --configuration=development",
    "lint": "ng lint",
    "fix": "ng lint --fix",
    "test:all": "npm run knip && npm run lint && npm run typecheck && npm test:ci && npm run extract-i18n",
    "test:deployment-configuration": "node --env-file=.env ./_test-deployment-configuration.mjs",
    "test:ci": "npm test -- --no-watch --no-progress --browsers=ChromeHeadless",
    "test:coverage": "npm run test:ci -- --code-coverage",
    "test": "ng test",
    "upgrade:angular": "ng update @angular/core @angular/cli --create-commits",
    "verify:deployment-configuration": "node --env-file=.env ./_verify-deployment-configuration.mjs",
    "verify:build": "node --env-file=.env ./_verify-build.mjs",
    "typecheck": "npx tsc --noEmit",
    "extract-i18n": "ng extract-i18n",
    "knip": "knip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/common": "^20.1.2",
    "@angular/compiler": "^20.1.2",
    "@angular/core": "^20.1.2",
    "@angular/forms": "^20.1.2",
    "@angular/platform-browser": "^20.1.2",
    "@angular/platform-browser-dynamic": "^20.1.2",
    "@angular/router": "^20.1.2",
    "@azure/msal-angular": "3.1.0",
    "@azure/msal-browser": "3.28.0",
    "@microsoft/applicationinsights-web": "^3.3.8",
    "@primeuix/themes": "^1.2.1",
    "@tanstack/angular-query-experimental": "^5.80.7",
    "angular-mentions": "^1.5.0",
    "angular-svg-icon": "^19.1.1",
    "chart.js": "^4.4.9",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^4.1.0",
    "filesize": "^9.0.11",
    "konami-code-js": "^0.8.3",
    "lokalise-file-exchange": "^3.1.0",
    "ngx-filesize": "^3.0.5",
    "ngx-matomo-client": "^8.0.0",
    "papaparse": "^5.5.3",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.0",
    "radashi": "^12.5.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.2",
    "@angular/localize": "^20.1.2",
    "@azure/static-web-apps-cli": "^2.0.6",
    "@microsoft/applicationinsights-debugplugin-js": "^3.3.8",
    "@prettier/plugin-xml": "^3.4.2",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@types/jasmine": "~5.1.8",
    "@types/konami-code-js": "^0.8.3",
    "@types/node": "^20.19.0",
    "@types/papaparse": "^5.3.16",
    "angular-eslint": "^20.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.2.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "knip": "^5.62.0",
    "ng-extract-i18n-merge": "^2.15.1",
    "postcss": "^8.5.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shx": "^0.4.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "overrides": {
    "ngx-filesize": {
      "@angular/animations": "$@angular/animations",
      "@angular/core": "$@angular/core",
      "@angular/common": "$@angular/common"
    }
  }
}
