{
  "name": "portalicious",
  "scripts": {
    "ng": "ng",
    "postinstall": "cd ../../services/121-service && npm run install:dependencies-for-portal",
    "set-env-variables": "node ./_set-env-variables.js",
    "preserve:static": "shx cp www/index.html www/404.html",
    "serve:static": "npx http-server ./www/ -c30 --gzip --brotli --port 8088",
    "prestart": "npm run set-env-variables -- env=development",
    "start": "ng serve",
    "start:debug-production": "npm run build:prod && npm run serve:static",
    "prebuild": "npm run set-env-variables -- env=production",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration=production",
    "postbuild": "shx mv www/browser/* www/",
    "watch": "ng build --watch --configuration=development",
    "lint": "ng lint",
    "fix": "ng lint --fix",
    "test:all": "npm run lint && npm test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@microsoft/applicationinsights-web": "^3.3.0",
    "@tanstack/angular-query-experimental": "5.49.1",
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "lodash": "^4.17.21",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.5",
    "@angular/cli": "^18.0.5",
    "@angular/compiler-cli": "^18.0.0",
    "@microsoft/applicationinsights-debugplugin-js": "^3.3.0",
    "@tanstack/eslint-plugin-query": "^5.50.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.6",
    "angular-eslint": "^18.1.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.7.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.38",
    "prettier": "3.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.4.2",
    "typescript-eslint": "^8.0.0-alpha.39"
  }
}
