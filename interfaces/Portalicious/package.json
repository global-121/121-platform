{
  "name": "portalicious",
  "scripts": {
    "ng": "ng",
    "postinstall": "cd ../../services/121-service && npm run install:dependencies-for-portal",
    "set-env-variables": "node ./_set-env-variables.js",
    "build:manual-logout": "node ./_build-manual-logout.js",
    "serve:static": "swa start ./www --port 8088",
    "prestart": "npm run set-env-variables -- env=development && cd ../../services/121-service && npx tsc -b",
    "start": "ng serve",
    "start:debug-production": "npm run build:prod && npm run serve:static",
    "prebuild": "npm run set-env-variables -- env=production",
    "build": "ng build --localize",
    "build:prod": "npm run build -- --configuration=production",
    "postbuild": "shx mv www/browser/* www/ && shx touch www/index.html && npm run build:manual-logout",
    "watch": "ng build --watch --configuration=development",
    "lint": "ng lint",
    "fix": "ng lint --fix",
    "test:all": "npm run lint && npm test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test": "ng test",
    "typecheck": "npx tsc --noEmit",
    "extract-i18n": "ng extract-i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.9",
    "@angular/common": "^18.2.9",
    "@angular/compiler": "^18.2.9",
    "@angular/core": "^18.2.9",
    "@angular/forms": "^18.2.9",
    "@angular/platform-browser": "^18.2.9",
    "@angular/platform-browser-dynamic": "^18.2.9",
    "@angular/router": "^18.2.9",
    "@microsoft/applicationinsights-web": "^3.3.0",
    "@tanstack/angular-query-experimental": "^5.52.0",
    "angular-mentions": "^1.5.0",
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "lodash": "^4.17.21",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.10",
    "@angular/cli": "^18.2.10",
    "@angular/compiler-cli": "^18.2.9",
    "@angular/localize": "^18.2.9",
    "@azure/static-web-apps-cli": "^2.0.1",
    "@microsoft/applicationinsights-debugplugin-js": "^3.3.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@savvywombat/tailwindcss-grid-areas": "^4.0.0",
    "@tanstack/eslint-plugin-query": "^5.50.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.6",
    "angular-eslint": "^18.1.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.7.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-extract-i18n-merge": "^2.12.0",
    "postcss": "^8.4.38",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.4.2",
    "typescript-eslint": "^8.0.0-alpha.39"
  }
}
