# These environment variables can be used to configure the production build
# See usage in: src/environments/environment.prod.ts.template.js
#
# You can set them in a file, when you copy this example file to `.env`
#     cp .env.example .env
#

NG_PRODUCTION=true

# Feature-switches:
NG_IS_DEBUG=false
NG_SHOW_DEBUG=false
NG_USE_ANIMATION=true
NG_USE_SERVICE_WORKER=false

# APIs:
NG_URL_121_SERVICE_API=

# Third-party tokens:
# Application Insights
NG_AI_IKEY=
NG_AI_ENDPOINT=
