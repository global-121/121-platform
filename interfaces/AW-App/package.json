{
  "name": "aw-app",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "interfaces/AW-App"
  },
  "scripts": {
    "ionic": "ionic",
    "ng": "ng",
    "start": "ng serve --port 0",
    "set-env-variables": "node ./_set-env-variables.js",
    "prebuild": "npm run set-env-variables",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production",
    "postbuild:prod": "npm run check:icons-in-use && npm run compress",
    "compress": "gzipper compress --skip-compressed --remove-larger --gzip --gzip-level 9 --brotli ./www/",
    "check:icons-in-use": "node ./_check-icons-in-use.js",
    "build:debug": "npm run build -- --configuration production -- --base-href=\"/aw-app/\"",
    "postbuild:debug": "shx cp www/index.html www/404.html && shx mv www \"aw-app\" && shx mkdir www && shx mv \"aw-app\" www/",
    "serve:static": "npx http-server ./www/ -c3600 --gzip --brotli --port 8088 --proxy http://localhost:8088/aw-app/?",
    "start:debug-production": "npm run build:debug && npm run serve:static",
    "test": "ng test --configuration=ci",
    "test:dev": "ng test",
    "lint:syntax": "prettier --check \"**/*.{md,js,ts,scss,html}\"",
    "lint": "npm run lint:syntax && ng lint",
    "fix:syntax": "npm run lint:syntax -- --write"
  },
  "private": true,
  "engines": {
    "node": ">=14",
    "npm": "^6"
  },
  "engineStrict": true,
  "dependencies": {
    "@angular/common": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@angular/service-worker": "^12.2.16",
    "@ionic/angular": "^6.3.3",
    "@ionic/cli": "^6.20.3",
    "@ionic/storage": "2.2.0",
    "@microsoft/applicationinsights-web": "^2.8.9",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "core-js": "2.6.8",
    "rxjs": "^6.6.2",
    "tslib": "^2.4.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.18",
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-devkit/core": "^12.2.18",
    "@angular-devkit/schematics": "^12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler": "^12.2.16",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "@types/jasmine": "^4.0.2",
    "@types/node": "14.x",
    "codelyzer": "^6.0.0",
    "dotenv": "^16.0.3",
    "gzipper": "^7.2.0",
    "http-server": "^14.1.0",
    "jasmine-core": "^3.99.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.18",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "^3.2.0",
    "shx": "^0.3.3",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
