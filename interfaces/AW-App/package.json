{
  "name": "aw-app",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "interfaces/AW-App"
  },
  "scripts": {
    "ionic": "ionic",
    "ng": "ng",
    "start": "ng serve --port 0",
    "set-env-variables": "node ./_set-env-variables.js",
    "prebuild": "npm run set-env-variables",
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "postbuild:prod": "npm run convert:styles-to-async && npm run compress:gzip && npm run compress:brotli",
    "compress:gzip": "gzipper compress --skip-compressed --remove-larger --level 9 ./www/",
    "compress:brotli": "gzipper compress --skip-compressed --remove-larger --brotli ./www/",
    "convert:styles-to-async": "node ./_convert-styles-to-async.js",
    "build:debug": "npm run build:prod -- --base-href=\"/aw-app/\"",
    "copy:sw-debug": "shx cp www/index.html www/404.html && shx mv www \"aw-app\" && shx mkdir www && shx mv \"aw-app\" www/",
    "serve:prod": "npx http-server ./www/ -c-1",
    "debug:service-worker": "npm run build:debug && npm run copy:sw-debug && npm run serve:prod",
    "test": "ng test --no-watch --browsers=ChromeHeadless",
    "test:dev": "ng test",
    "lint:translations": "npx ngx-translate-lint --config=ngx-translate-lint.config.json",
    "lint:syntax": "prettier --check \"**/*.{md,js,ts,scss}\"",
    "lint": "ng lint && npm run lint:syntax",
    "fix:syntax": "npm run lint:syntax -- --write"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/pwa": "^0.13.10",
    "@angular/router": "^7.2.16",
    "@angular/service-worker": "^7.2.16",
    "@auth0/angular-jwt": "^4.2.0",
    "@ionic/angular": "^4.11.8",
    "@ionic/cli": "^6.11.8",
    "@ionic/storage": "2.2.0",
    "@microsoft/applicationinsights-web": "^2.5.9",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@zxing/ngx-scanner": "^2.0.1",
    "core-js": "2.6.8",
    "rxjs": "^6.6.2",
    "tslib": "^1.12.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.13.10",
    "@angular-devkit/build-angular": "^0.13.10",
    "@angular-devkit/core": "^7.3.10",
    "@angular-devkit/schematics": "^7.3.10",
    "@angular/cli": "^7.3.10",
    "@angular/compiler": "^7.2.16",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@ionic/angular-toolkit": "^1.5.1",
    "@types/jasmine": "^3.7.4",
    "@types/node": "12.x",
    "codelyzer": "^5.2.2",
    "dotenv": "^10.0.0",
    "gzipper": "^5.0.0",
    "http-server": "^13.0.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "prettier": "2.4.0",
    "prettier-plugin-organize-imports": "^2.1.0",
    "shx": "^0.3.3",
    "tslint": "~5.20.1",
    "typescript": "~3.2.4"
  }
}
