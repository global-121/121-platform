{
  "name": "pa-app",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "interfaces/PA-App"
  },
  "scripts": {
    "ng": "ng",
    "ionic": "ionic",
    "cordova": "cordova",
    "start": "ng serve --port 0",
    "dev:on-device": "npm run ionic -- cordova run android --device --consolelogs --livereload",
    "dev:local-android": "npm run ionic -- cordova run android --emulator --consolelogs --livereload",
    "dev:local-ios": "npm run ionic -- cordova run ios --consolelogs --livereload",
    "dev:local": "npm run ionic -- cordova run browser --consolelogs --livereload",
    "generate-assets-audio": "node ./_generate-assets-audio.js",
    "check-assets-translations": "node ./_check-assets-translations.js",
    "set-env-variables": "node ./_set-env-variabes.js",
    "prebuild": "npm run set-env-variables && npm run check-assets-translations && npm run generate-assets-audio",
    "build": "ng build",
    "prebuild:native": "npm run prebuild",
    "build:native": "npm run ionic -- cordova build android ",
    "test": "ng test --no-watch --browsers=ChromeHeadless",
    "test:dev": "ng test",
    "test:all": "npm run lint && npm test",
    "lint:translations": "ngx-translate-lint --config=ngx-translate-lint.config.json",
    "lint:syntax": "prettier --check \"**/*.{md,js,ts,scss}\"",
    "lint": "ng lint && npm run lint:syntax && npm run lint:translations",
    "fix:syntax": "npm run lint:syntax -- --write",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "@auth0/angular-jwt": "^4.2.0",
    "@ionic-native/core": "^5.16.0",
    "@ionic-native/splash-screen": "^5.23.0",
    "@ionic-native/status-bar": "^5.16.0",
    "@ionic/angular": "^4.11.8",
    "@ionic/cli": "^6.11.8",
    "@ionic/storage": "2.2.0",
    "@microsoft/applicationinsights-web": "^2.5.8",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@zxing/ngx-scanner": "^2.0.1",
    "angularx-qrcode": "^1.6.4",
    "cordova-android": "^9.0.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^6.1.0",
    "core-js": "2.6.8",
    "howler": "^2.1.3",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.13.10",
    "@angular-devkit/build-angular": "^0.13.10",
    "@angular-devkit/core": "^7.3.10",
    "@angular-devkit/schematics": "^7.3.10",
    "@angular/cli": "^7.3.10",
    "@angular/compiler": "^7.2.16",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@ionic/angular-toolkit": "^1.5.1",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "10.x",
    "codelyzer": "^5.2.2",
    "cordova": "^10.0.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^4.1.3",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-res": "^0.15.1",
    "dotenv": "^8.2.0",
    "ffmpeg-cli": "^2.7.1",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "native-run": "^1.0.0",
    "ngx-translate-lint": "^1.4.0",
    "prettier": "2.2.0",
    "prettier-plugin-organize-imports": "^1.1.1",
    "protractor": "^7.0.0",
    "ts-node": "~9.1.0",
    "tslint": "~5.20.1",
    "typescript": "~3.2.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-device": {}
    },
    "platforms": [
      "browser",
      "android",
      "ios"
    ]
  }
}
