{
  "name": "ho-portal",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "interfaces/HO-Portal"
  },
  "scripts": {
    "build:debug": "npm run build:prod -- --base-href=\"/ho-portal/\"",
    "build:prod": "npm run build -- --configuration production",
    "build": "ng build",
    "compress": "gzipper compress --skip-compressed --remove-larger --gzip --gzip-level 9 --brotli ./www/",
    "fix": " ng lint --fix && npm run lint:syntax -- --write",
    "lint:syntax": "npx prettier --check \"**/*.{md,js,json,ts,scss,html}\"",
    "lint": "npm run lint:syntax && ng lint",
    "postbuild:debug": "shx cp www/index.html www/404.html && shx mv www \"ho-portal\" && shx mkdir www && shx mv \"ho-portal\" www/",
    "postbuild:prod": "npm run compress",
    "prebuild": "npm run set-env-variables",
    "serve:static": "npx http-server ./www/ -c3600 --gzip --brotli --port 8088 --proxy http://localhost:8088/ho-portal/?",
    "set-env-variables": "node ./_set-env-variables.js",
    "start:debug-production": "npm run build:debug && npm run serve:static",
    "start": "ng serve --port 0",
    "test:dev": "ng test",
    "test": "ng test --configuration=ci",
    "upgrade:angular": "ng update @angular/core @angular/cli @angular-eslint/schematics --create-commits"
  },
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "engineStrict": true,
  "dependencies": {
    "@angular/animations": "^15.2.4",
    "@angular/common": "^15.2.4",
    "@angular/core": "^15.2.4",
    "@angular/forms": "^15.2.4",
    "@angular/platform-browser": "^15.2.4",
    "@angular/platform-browser-dynamic": "^15.2.4",
    "@angular/router": "^15.2.4",
    "@angular/service-worker": "^15.2.4",
    "@ionic/angular": "^6.6.2",
    "@ionic/cli": "^6.20.9",
    "@microsoft/applicationinsights-web": "^2.8.11",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@swimlane/ngx-charts": "^20.1.2",
    "@swimlane/ngx-datatable": "^20.1.0",
    "core-js": "2.6.8",
    "file-saver": "^2.0.2",
    "ngx-popperjs": "^15.0.4",
    "rxjs": "^7.4.0",
    "tslib": "^2.4.1",
    "xlsx": "^0.18.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1502.4",
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-devkit/core": "^15.2.4",
    "@angular-devkit/schematics": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.4",
    "@angular/compiler": "^15.2.4",
    "@angular/compiler-cli": "^15.2.4",
    "@angular/language-service": "^15.2.4",
    "@ionic/angular-toolkit": "^8.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "codelyzer": "^6.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jsdoc": "^40.1.0",
    "gzipper": "^7.2.0",
    "http-server": "^14.1.0",
    "jasmine-core": "^4.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "shx": "^0.3.4",
    "typescript": "~4.9.5"
  }
}
