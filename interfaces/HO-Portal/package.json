{
  "name": "ho-portal",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "interfaces/HO-Portal"
  },
  "scripts": {
    "ionic": "ionic",
    "ng": "ng",
    "start": "ng serve --port 0",
    "set-env-variables": "node ./_set-env-variables.js",
    "prebuild": "npm run set-env-variables",
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "postbuild:prod": "npm run compress:gzip && npm run compress:brotli",
    "compress:gzip": "gzipper compress --skip-compressed --remove-larger --level 9 ./www/",
    "compress:brotli": "gzipper compress --skip-compressed --remove-larger --brotli ./www/",
    "build:debug": "npm run build:prod -- --base-href=\"/ho-portal/\"",
    "postbuild:debug": "shx cp www/index.html www/404.html && shx mv www \"ho-portal\" && shx mkdir www && shx mv \"ho-portal\" www/",
    "serve:static": "npx http-server ./www/ -c3600 --gzip --brotli --port 8088 --proxy http://localhost:8088/ho-portal/?",
    "start:debug-production": "npm run build:debug && npm run serve:static",
    "test": "ng test --no-watch --browsers=ChromeHeadless",
    "test:dev": "ng test",
    "lint:syntax": "prettier --check \"**/*.{md,js,ts,scss}\"",
    "lint": "npm run lint:syntax && ng lint",
    "fix:syntax": "npm run lint:syntax -- --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@angular/service-worker": "^11.2.14",
    "@ionic/angular": "^5.8.4",
    "@ionic/cli": "^6.11.8",
    "@microsoft/applicationinsights-web": "^2.7.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "@popperjs/core": "~2.4.4",
    "@swimlane/ngx-charts": "^17.0.1",
    "@swimlane/ngx-datatable": "^19.0.0",
    "core-js": "2.6.8",
    "file-saver": "^2.0.2",
    "ngx-popperjs": "^11.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "xlsx": "^0.18.2",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1102.19",
    "@angular-devkit/build-angular": "^0.1102.19",
    "@angular-devkit/core": "^11.2.19",
    "@angular-devkit/schematics": "^11.2.19",
    "@angular/cli": "^11.2.19",
    "@angular/compiler": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^4.0.2",
    "@types/node": "^12.x",
    "codelyzer": "^5.1.2",
    "dotenv": "^16.0.0",
    "gzipper": "^5.0.0",
    "http-server": "^14.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.18",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.5.0",
    "prettier-plugin-organize-imports": "^2.1.0",
    "shx": "^0.3.3",
    "tslint": "~6.1.0",
    "typescript": "^4.0.8"
  }
}
