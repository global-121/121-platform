{
  "name": "ho-portal",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "interfaces/HO-Portal"
  },
  "scripts": {
    "ionic": "ionic",
    "ng": "ng",
    "start": "ng serve --port 0",
    "set-env-variables": "node ./_set-env-variables.js",
    "prebuild": "npm run set-env-variables",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production",
    "postbuild:prod": "npm run compress",
    "compress": "gzipper compress --skip-compressed --remove-larger --gzip --gzip-level 9 --brotli ./www/",
    "build:debug": "npm run build:prod -- --base-href=\"/ho-portal/\"",
    "postbuild:debug": "shx cp www/index.html www/404.html && shx mv www \"ho-portal\" && shx mkdir www && shx mv \"ho-portal\" www/",
    "serve:static": "npx http-server ./www/ -c3600 --gzip --brotli --port 8088 --proxy http://localhost:8088/ho-portal/?",
    "start:debug-production": "npm run build:debug && npm run serve:static",
    "test": "ng test --configuration=ci",
    "test:dev": "ng test",
    "lint:syntax": "npx prettier --check \"**/*.{md,js,ts,scss,html}\"",
    "lint": "npm run lint:syntax && ng lint",
    "fix:syntax": "npm run lint:syntax -- --write"
  },
  "private": true,
  "engines": {
    "node": ">=14",
    "npm": "^6"
  },
  "engineStrict": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@angular/service-worker": "^14.2.12",
    "@ionic/angular": "^6.6.1",
    "@ionic/cli": "^6.20.3",
    "@microsoft/applicationinsights-web": "^2.8.10",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "~2.11.5",
    "@swimlane/ngx-charts": "^20.1.2",
    "@swimlane/ngx-datatable": "^20.0.0",
    "core-js": "2.6.8",
    "file-saver": "^2.0.2",
    "ngx-popperjs": "^13.3.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.4.1",
    "xlsx": "^0.18.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.10",
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-devkit/core": "^14.2.10",
    "@angular-devkit/schematics": "^14.2.10",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler": "^14.2.12",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@ionic/angular-toolkit": "^5.0.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^4.0.2",
    "@types/node": "^14.x",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.51.0",
    "@typescript-eslint/parser": "5.55.0",
    "codelyzer": "^6.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.8.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "gzipper": "^7.2.0",
    "http-server": "^14.1.0",
    "jasmine-core": "^3.99.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.18",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
