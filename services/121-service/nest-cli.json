{
  "compilerOptions": {
    "assets": [
      {
        "include": "**/*.json",
        "outDir": "dist",
        "watchAssets": true
      },
      {
        "include": "**/*.csv",
        "outDir": "dist",
        "watchAssets": true
      }
    ],
    "watchAssets": true,
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "typeCheck": true,
      "options": {
        "extensions": [".js", ".ts", ".jsx", ".tsx"],
        "swcrcPath": ".swcrc"
      }
    }
  }
}
