{
  "name": "121-service",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "services/121-service"
  },
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:seed-data && npm run lint:code && npm run lint:syntax",
    "lint:code": "eslint \"src/**/*.ts\"",
    "fix:code": "npm run lint:code -- --fix",
    "lint:seed-data": "prettier --check \"seed-data/**/*.json\"",
    "lint:syntax": "prettier --check \"**/*.{md,ts,js,json,yml}\"",
    "fix:syntax": "npm run lint:code -- --fix && npm run lint:syntax -- --write && npm run lint:seed-data -- --write",
    "prestart": "npm run migration:run && ts-node src/scripts seed-prod",
    "start": "node index.js",
    "prestart:dev": "npm install --no-fund --no-audit --no-save && npm run prestart",
    "start:dev": "tsc-watch --noClear --onSuccess \"  node index.js  \" --onFailure \"echo There was a problem with the build!\" -p tsconfig.json",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "seed:dev": "ts-node src/scripts seed-dev",
    "seed:program-demo": "ts-node src/scripts seed-program-demo",
    "seed:program-validation": "ts-node src/scripts seed-program-validation",
    "seed:program-pilot-nl": "ts-node src/scripts seed-program-pilot-nl",
    "seed:program-pilot-nl2": "ts-node src/scripts seed-program-pilot-nl-2",
    "seed:program-pilot-eth": "ts-node src/scripts seed-program-eth",
    "seed:program-pilot-lbn": "ts-node src/scripts seed-program-lbn",
    "pretest": "npm run lint",
    "test": "echo 'No tests defined. Run `npm run test:e2e` to run API-tests locally.' && exit 0",
    "test:e2e": "jest --runInBand",
    "test:watch": "npm run test:e2e -- --watchAll",
    "test:coverage": "npm run test:e2e -- --coverage --coverageDirectory=coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create --",
    "migration:generate": "npm run typeorm migration:generate -- -d ./appdatasource.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./appdatasource.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./appdatasource.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.1.1",
    "@nestjs/testing": "^8.4.7",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^8.1.3",
    "africastalking": "^0.5.5",
    "applicationinsights": "^1.8.8",
    "bwip-js": "^2.0.11",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "easy-soap-request": "^3.5.0",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "openid-client": "^5.4.0",
    "pg": "^8.8.0",
    "rxjs": "^7.2.0",
    "soap": "^0.33.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "twilio": "^3.36.0",
    "typeorm": "^0.3.6",
    "typescript": "^4.7.4",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "16.x",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "17.x",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^29.0.0",
    "nestjs-spelunker": "^1.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "tsc-watch": "^6.0.0",
    "yargs": "^17.0.1"
  }
}
