{
  "name": "121-service",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "services/121-service"
  },
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:seed-data && npm run lint:code && npm run lint:syntax",
    "lint:code": "eslint \"src/**/*.{ts,js}\" \"migration/*.ts\" \"test/**/*.ts\"",
    "fix:code": "npm run lint:code -- --fix",
    "lint:seed-data": "prettier --check \"seed-data/**/*.json\"",
    "fix:seed-data": "npm run lint:seed-data -- --write",
    "lint:syntax": "prettier --check \"**/*.{md,ts,js,json,yml}\"",
    "fix:syntax": "npm run fix:code && npm run lint:syntax -- --write",
    "start": "GLOBAL_121_VERSION=$(test -f \"VERSION.txt\" && cat VERSION.txt || echo $GLOBAL_121_VERSION)  node index.js",
    "prestart:dev": "npm install --no-fund --no-audit --no-save",
    "start:dev": "tsc-watch --noClear --onSuccess \"  node --inspect=0.0.0.0 index.js  \" --onFailure \"echo There was a problem with the build!\" -p tsconfig.json",
    "seed:dev": "ts-node src/scripts seed-dev",
    "pretest": "npm run lint",
    "test": "echo 'No automated tests. Run `npm run test:e2e:all` to run API-tests locally. Run `npm run test:unit:all` to run unit-tests locally.' && exit 0",
    "test:unit:all": "jest --runInBand --config=./jest.unit.config.js",
    "test:unit:watch": "npm run test:unit:all -- --watchAll",
    "test:unit:coverage": "npm run test:unit:all -- --coverage --coverageDirectory=coverage/unit",
    "test:e2e:all": "node --expose-gc node_modules/.bin/jest --config=jest.e2e.config.js --runInBand --detectOpenHandles --logHeapUsage",
    "test:e2e:watch": "npm run test:e2e:all -- --watchAll",
    "test:e2e:coverage": "npm run test:e2e:all -- --coverage --coverageDirectory=coverage/e2e",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create --",
    "migration:generate": "npm run typeorm migration:generate -- -d ./appdatasource.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./appdatasource.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./appdatasource.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/testing": "^10.3.7",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "africastalking": "^0.5.5",
    "applicationinsights": "^2.9.5",
    "bull": "^4.11.4",
    "bwip-js": "^2.0.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "easy-soap-request": "^5.5.0",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nestjs-paginate": "^8.4.0",
    "nestjs-spelunker": "^1.3.0",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "passport-jwt": "^4.0.1",
    "pg": "^8.8.0",
    "rxjs": "^7.8.1",
    "soap": "^0.33.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^5.0.1",
    "typeorm": "^0.3.17",
    "typescript": "^4.9.5",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/jest": "^29.5.12",
    "@types/passport-azure-ad": "^4.3.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.19.26",
    "@types/supertest": "^6.0.2",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-ci-spec-reporter": "^1.0.3",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.0.4",
    "yargs": "^17.7.2"
  }
}
