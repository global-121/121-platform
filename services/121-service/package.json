{
  "name": "121-service",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "services/121-service"
  },
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:seed-data && npm run lint:code && npm run lint:syntax",
    "lint:code": "eslint \"src/**/*.{ts,js}\"",
    "fix:code": "npm run lint:code -- --fix",
    "lint:seed-data": "prettier --check \"seed-data/**/*.json\"",
    "lint:syntax": "prettier --check \"**/*.{md,ts,js,json,yml}\"",
    "fix:syntax": "npm run lint:code -- --fix && npm run lint:syntax -- --write && npm run lint:seed-data -- --write",
    "setup": "ts-node src/scripts seed-prod",
    "start": "GLOBAL_121_VERSION=$(test -f \"VERSION.txt\" && cat VERSION.txt || echo $GLOBAL_121_VERSION)  node index.js",
    "prestart:dev": "npm install --no-fund --no-audit --no-save",
    "start:dev": "tsc-watch --noClear --onSuccess \"  node --inspect index.js  \" --onFailure \"echo There was a problem with the build!\" -p tsconfig.json",
    "seed:dev": "ts-node src/scripts seed-dev",
    "seed:program-demo": "ts-node src/scripts seed-program-demo",
    "seed:program-validation": "ts-node src/scripts seed-program-validation",
    "seed:program-pilot-nl": "ts-node src/scripts seed-program-pilot-nl",
    "seed:program-pilot-nl2": "ts-node src/scripts seed-program-pilot-nl-2",
    "seed:program-pilot-eth": "ts-node src/scripts seed-program-eth",
    "seed:program-pilot-lbn": "ts-node src/scripts seed-program-lbn",
    "seed:program-pilot-dorcas-eth": "ts-node src/scripts seed-program-dorcas-eth",
    "seed:program-pilot-krcs": "ts-node src/scripts seed-program-krcs",
    "pretest": "npm run lint",
    "test": "echo 'No automated tests. Run `npm run test:e2e:all` to run API-tests locally. Run `npm run test:unit:all` to run unit-tests locally.' && exit 0",
    "test:unit:all": "jest --runInBand --config=./jest.unit.config.js",
    "test:unit:watch": "npm run test:unit:all -- --watchAll",
    "test:unit:coverage": "npm run test:unit:all -- --coverage --coverageDirectory=coverage/unit",
    "test:e2e:all": "jest --runInBand --config=./jest.e2e.config.js",
    "test:e2e:watch": "npm run test:e2e:all -- --watchAll",
    "test:e2e:coverage": "npm run test:e2e:all -- --coverage --coverageDirectory=coverage/e2e",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create --",
    "migration:generate": "npm run typeorm migration:generate -- -d ./appdatasource.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./appdatasource.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./appdatasource.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/testing": "^10.2.7",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "africastalking": "^0.5.5",
    "applicationinsights": "^2.9.0",
    "bull": "^4.11.4",
    "bwip-js": "^2.0.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "easy-soap-request": "^3.5.0",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nestjs-paginate": "^8.4.0",
    "nestjs-spelunker": "^1.3.0",
    "openid-client": "^5.6.1",
    "pg": "^8.8.0",
    "rxjs": "^7.8.1",
    "soap": "^0.33.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^3.84.1",
    "typeorm": "^0.3.6",
    "typescript": "^4.9.5",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.18.8",
    "@types/supertest": "^2.0.15",
    "@types/yargs": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-watch": "^6.0.4",
    "yargs": "^17.7.2"
  }
}
