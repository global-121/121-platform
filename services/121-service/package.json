{
  "name": "121-service",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/global-121/121-platform.git",
    "directory": "services/121-service"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "npm run lint -- --fix",
    "build": "nest build -p tsconfig.json",
    "start": "GLOBAL_121_VERSION=$(test -f \"VERSION.txt\" && cat VERSION.txt || echo $GLOBAL_121_VERSION)  node --max-old-space-size=4096 dist/main.js",
    "start:dev": "NODE_OPTIONS='--max-old-space-size=4096' nodemon --watch src/**/*.json --exec npm run start:dev:nest",
    "start:dev:nest": "nest start -r tsconfig-paths/register -d 0.0.0.0:9229 -w -p tsconfig.json",
    "start:dev:windows": "nodemon --legacy-watch --watch src --ext ts,js,json --exec 'npm run start:dev:nest:windows'",
    "start:dev:nest:windows": "nest start -r tsconfig-paths/register -d 0.0.0.0:9229 -p tsconfig.json",
    "prestart:dev": "npm install --no-fund --no-audit --no-save",
    "seed:dev": "ts-node -r tsconfig-paths/register src/scripts seed-dev",
    "test": "echo 'No automated tests. Run `npm run test:e2e:all` to run API-tests locally. Run `npm run test:unit:all` to run unit-tests locally.' && exit 0",
    "test:unit:all": "jest --runInBand --config=./jest.unit.config.js",
    "test:unit:watch": "npm run test:unit:all -- --watchAll",
    "test:unit:coverage": "npm run test:unit:all -- --coverage --coverageDirectory=coverage/unit",
    "test:e2e:all": "node -r tsconfig-paths/register --expose-gc node_modules/.bin/jest --config=jest.e2e.config.js --runInBand --detectOpenHandles --logHeapUsage",
    "test:e2e:watch": "npm run test:e2e:all -- --watchAll",
    "test:e2e:coverage": "npm run test:e2e:all -- --coverage --coverageDirectory=coverage/e2e",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create -- -d ./src/datasource-manage-migrations.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/datasource-manage-migrations.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/datasource-manage-migrations.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/datasource-manage-migrations.ts",
    "knip": "knip",
    "knip:fix": "knip --fix-type files,exports,types"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/testing": "^10.3.9",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "africastalking": "^0.6.7",
    "applicationinsights": "^2.9.5",
    "bull": "^4.14.0",
    "bwip-js": "^2.0.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "easy-soap-request": "^5.6.1",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nestjs-paginate": "^8.6.2",
    "nestjs-spelunker": "^1.3.0",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "rxjs": "^7.8.1",
    "soap": "^1.0.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^5.0.4",
    "typeorm": "^0.3.17",
    "typescript": "^5.5.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@compodoc/compodoc": "^1.1.25",
    "@nestjs/cli": "^10.3.2",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@types/cookie-parser": "^1.4.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.5",
    "@types/node": "^18.19.39",
    "@types/passport-azure-ad": "^4.3.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "jest": "^29.7.0",
    "jest-ci-spec-reporter": "^1.0.3",
    "knip": "^5.23.1",
    "nodemon": "^3.1.1",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.2.0",
    "yargs": "^17.7.2"
  }
}
